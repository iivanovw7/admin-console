{
  "plugins": [
    "@getify/proper-ternary"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "@getify/proper-ternary/nested": "error",
    "@getify/proper-ternary/parens": "error"
  }
}